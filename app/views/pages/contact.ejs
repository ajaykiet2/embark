<section class="contact-form-section">
	<div class="inner-container container">
		<div class="acr-title">
			<div class="inner-box">Contact <span>Form</span></div>
		</div>
		<div class="contact-form-box">
			<form action="#">
				<div class="field-row row">
					<div class="col-md-6">
						<label for="contact-name">Name* :</label>
						<div class="input-container">
							<input type="text" id="contact-name" name="contact-name" required></div>
					</div>
					<div class="col-md-6">
						<label for="contact-email">Email* :</label>
						<div class="input-container">
							<input type="email" id="contact-email" name="contact-email" required>
						</div>
					</div>
				</div>
				<div class="field-row row">
					<div class="col-md-6">
						<label for="contact-phone">Phone :</label>
						<div class="input-container">
							<input type="text" id="contact-phone" name="contact-phone">
						</div>
					</div>
					<div class="col-md-6">
						<label for="contact-website">Website :</label>
						<div class="input-container">
							<input type="email" id="contact-website" name="contact-website">
						</div>
					</div>
				</div>
				<div class="field-row row">
					<div class="col-md-12">
						<label for="contact-message">Message* :</label>
						<div class="input-container">
							<textarea name="contact-message" id="contact-message" required></textarea>
						</div>
					</div>
				</div>
				<div class="btn-container">
					<button type="submit" class="btn-type-2">Send</button>
				</div>
			</form>
		</div>
	</div>
</section>
<section id="newsletter">
	<div class="inner-container">
		<div class="title">Stay Updated</div>
		<div class="subtitle">Join our <strong>54,727</strong> subscribers and get access to the latest news!</div>
		<form action="#" class="newsletter">
			<input type="text" placeholder="Your Email Address">
			<button type="submit">
				<i class="fa fa-send"></i>
			</button>
		</form>
	</div>
</section>
<%- include('../common/footer') %>